<h2>Find Album by Artist</h2>
<br>
<p><a href="/artists/new">Add new artist</a></p>
<br>

<table>
  <thead>
    <tr>
      <th>Artist</th>
      <th>Type</th>
    </tr>
  </thead>
  <tbody>
  <% @artists.each do |artist| %>
  <tr>
    <td><a href="/artists/<%= artist.id %>"><%= artist.name %></a></td>
    <td><%= artist.type %></td>
    <% end %>
  </tbody>
</table>
